# Define all coms in DAG streaming.
module_config {
    module_library : "/apollo/bazel-bin/modules/localization/ins550D/libins_localization_component.so"

    components {
      class_name : "INSLocalizationComponent"
      config {
        name : "ins_localization"
        config_file_path : "/apollo/modules/localization/conf/ins_localization.pb.txt"
        readers: [
          {
            channel: "/apollo/sensor/gnss/odometry"
            qos_profile: {
              depth : 10
            }
            pending_queue_size: 50
          }
        ]
      }
    }
}